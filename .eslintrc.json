{
	"rules": {
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"quotes": ["error", "single"],
		"max-len": ["error", 100],
		"curly": "error",
		"camelcase": ["error", {"properties": "never"}],
		"no-trailing-spaces": ["error"],
		"no-irregular-whitespace": ["error"]
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	}
}
